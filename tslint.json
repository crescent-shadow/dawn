{
  "extends": "tslint-microsoft-contrib",
  "rules": {
    "no-http-string": false,
    "no-inner-html": false,
    "no-relative-imports": false,
    "no-import-side-effect": [true, { "ignore-module": "(\\.scss)$" }],
    "insecure-random": false,
    "no-unsafe-any": false,
    "strict-boolean-expressions": false,
    "completed-docs": false
  }
}